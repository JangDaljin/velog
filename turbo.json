{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputLogs": "errors-only",
      "outputs": ["./dist/**/*"],
      "cache": true
    },
    "lint": {
      "dependsOn": ["^lint"],
      "outputLogs": "errors-only",
      "inputs": ["./src/**/*"],
      "cache": true
    }
  }
}
